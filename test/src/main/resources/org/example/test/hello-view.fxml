    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
   <Button mnemonicParsing="false" text="Hello 2" />
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
         <CheckBox layoutX="27.0" layoutY="41.0" mnemonicParsing="false" text="CheckBox" />
         <ColorPicker layoutX="38.0" layoutY="107.0" />
      </children>
   </AnchorPane>
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
